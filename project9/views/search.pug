html
    head
        style
            include style.css
    body
        a(href="/") Go Back
        table
            tr
                th First Name
                th Last Name
                th Email Address
                th Address
                th Age
            each user in myUser
                tr
                    td #{user.firstName}
                    td #{user.lastName}
                    td #{user.emailAddress}
                    td #{user.address}
                    td #{user.age}