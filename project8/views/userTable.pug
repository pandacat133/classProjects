html
    head
        style
            include style.css
    body
        a(href="/") Go Back
        table
            tr
                th User ID
                th First Name
                th Last Name
                th Email Address
                th Address
                th Age
                th Edit User
                th Delete User
            each user in users
                tr
                    td #{user.userId}
                    td #{user.firstName}
                    td #{user.lastName}
                    td #{user.emailAddress}
                    td #{user.address}
                    td #{user.age}
                    td
                        a(href=`/editUser/${user.userId}`) Edit User
                    td
                        a(href=`/deleteUser/${user.userId}`) Delete User